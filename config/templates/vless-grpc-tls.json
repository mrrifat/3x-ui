{
  "protocol": "vless",
  "listen": "0.0.0.0",
  "port": 443,
  "settings": {
    "clients": [],
    "decryption": "none",
    "fallbacks": []
  },
  "streamSettings": {
    "network": "grpc",
    "security": "tls",
    "grpcSettings": {
      "serviceName": "grpc",
      "multiMode": false
    },
    "tlsSettings": {
      "serverName": "",
      "certificates": [
        {
          "certificateFile": "",
          "keyFile": ""
        }
      ],
      "alpn": ["h2"]
    }
  },
  "sniffing": {
    "enabled": true,
    "destOverride": ["http", "tls", "quic"]
  }
}
